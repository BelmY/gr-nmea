<?xml version="1.0"?>
<block>
	<name>nmea_gpsd</name>
	<key>gps_nmea_gpsd</key>
	<category>gps</category>
	<import>import gps</import>
	<import>import socket</import>
	<make>gps.nmea_gpsd($host, $port)</make>
	<param>
		<name>GPSD Host</name>
		<key>host</key>
		<value>localhost</value>
		<type>string</type>
	</param>
	<param>
		<name>TCP port</name>
		<key>port</key>
		<value>2947</value>
		<type>int</type>
	</param>

	<source>
		<name>out</name>
		<type>byte</type>
	</source>
	<source>
		<name>gps_msg</name>
		<type>message</type>
		<optional>1</optional>
	</source>
</block>
